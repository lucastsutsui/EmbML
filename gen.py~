
import embml

datasets = ['aedes-sex']

for ds in datasets:
    path = '../SklearnModels/'
    embml.sklearnModel(path + ds + '/decisionTree.model', 'codes/sklearn/' + ds + '/decisionTree.c', opts='-c')
    embml.sklearnModel(path + ds + '/logisticRegression.model', 'codes/sklearn/' + ds + '/logistic.c', opts='-c')
    embml.sklearnModel(path + ds + '/mlp.model', 'codes/sklearn/' + ds + '/mlp.c', opts='-c')
    embml.sklearnModel(path + ds + '/mlp_sig.model', 'codes/sklearn/' + ds + '/mlp_sig.c', opts='-c')
    embml.sklearnModel(path + ds + '/svmLinear.model', 'codes/sklearn/' + ds + '/svmLinear.c', opts='-c')
    embml.sklearnModel(path + ds + '/svmPoly2.model', 'codes/sklearn/' + ds + '/svmPoly2.c', opts='-c')
    embml.sklearnModel(path + ds + '/svmRbf.model', 'codes/sklearn/' + ds + '/svmRbf.c', opts='-c')

    path = '../WekaModels/'
    embml.wekaModel(path + ds + '/j48.model', 'codes/weka/' + ds + '/decisionTree.c', opts='-c')
    embml.wekaModel(path + ds + '/logistic.model', 'codes/weka/' + ds + '/logistic.c', opts='-c')
    embml.wekaModel(path + ds + '/mlp.model', 'codes/weka/' + ds + '/mlp.c', opts='-c')
    embml.wekaModel(path + ds + '/svmLinear.model', 'codes/weka/' + ds + '/mlp_sig.c', opts='-c')
    embml.wekaModel(path + ds + '/svmPoly2.model', 'codes/weka/' + ds + '/svmPoly2.c', opts='-c')
    embml.wekaModel(path + ds + '/svmRbf.model', 'codes/weka/' + ds + '/svmRbf.c', opts='-c')
